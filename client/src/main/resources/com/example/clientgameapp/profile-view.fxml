<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import java.net.URL?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.clientgameapp.controllers.user.ProfileController"
            prefHeight="600.0" prefWidth="600.0">

    <stylesheets>
        <URL value="@styles/main.css"/>
    </stylesheets>
    <center>
        <Button styleClass="btn-main" text="Logout" onAction="#logout"/>
    </center>
    <right>
        <VBox styleClass="box-right-menu" spacing="10">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Button text="Create room" onAction="#navigateToRoomCreation" prefWidth="100"/>
            <Button text="Find room" onAction="#navigateToRoomList" prefWidth="100"/>
        </VBox>
    </right>

</BorderPane>
